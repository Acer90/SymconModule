    "actions":
    [
        {
            "type": "RowLayout",
            "items": [
                { "type": "Button", "label": "Get Link (Local)", "onClick": " echo SymconJSLiveDoughnutPie_GetLink($id, True);" },
                { "type": "Button", "label": "Get Link (Remote)", "onClick": " echo SymconJSLiveDoughnutPie_GetLink($id, False);" }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Configuration",
            "items": [
                { "type": "Label", "caption": "Load from other Modul"},
                {
                    "type": "RowLayout",
                    "items": [
                        {"type": "SelectObject", "name": "ObjectID", "caption": "Modul"},
                        { "type": "Button", "label": "Load", "onClick": " echo SymconJSLiveDoughnutPie_LoadOtherConfiguration($id, $ObjectID);"}
                    ]
                },
                { "type": "Label", "caption": "Load from File"},
                {
                    "type": "RowLayout",
                    "items": [
                        {"type": "SelectFile", "name": "Config_fileData", "caption": "File"},
                        { "type": "Button", "label": "Load", "onClick": " echo SymconJSLiveDoughnutPie_LoadConfigurationFile($id, $Config_fileData);"}
                    ]
                },
                { "type": "Button", "label": "Export", "onClick": " echo SymconJSLiveDoughnutPie_GetConfigurationLink($id);" }
            ]
        }
