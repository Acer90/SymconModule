{
    "elements":
    [
        { "type": "Select", "name": "initialView", "caption": "initial View", "width": "150px",
            "options": [
                { "caption": "dayGridMonth", "value": "dayGridMonth" },
                { "caption": "timeGridDay", "value": "timeGridDay" },
                { "caption": "timeGridWeek", "value": "timeGridWeek" },
                { "caption": "listDay", "value": "listDay" },
                { "caption": "listWeek", "value": "listWeek" },
                { "caption": "listMonth", "value": "listMonth" },
                { "caption": "listYear", "value": "listYear" },
                { "caption": "dayGridDay", "value": "dayGridDay" },
                { "caption": "dayGridWeek", "value": "dayGridWeek" }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Buttons",
            "items": [
                { "type": "CheckBox", "name": "buttons_display", "caption": "Display"},
                {
                    "type": "RowLayout",
                    "items": [
                        { "type": "Label", "caption": "Font:", "width": "100px" },
                        { "type": "NumberSpinner", "name": "buttons_fontSize", "caption": "Size", "width": "100px", "suffix": "em", "minimum": 0, "digits": 2},
                        { "type": "SelectColor", "name": "buttons_fontColor", "caption": "Color", "width": "150px"},
                        { "type": "Select", "name": "buttons_fontFamily", "caption": "Family", "width": "150px",
                            "options": [
                                { "caption": "-Default-", "value": "" },
                                { "caption": "Amatic SC", "value": "Amatic SC" },
                                { "caption": "Architects Daughter", "value": "Architects Daughter" },
                                { "caption": "Bungee Hairline", "value": "Bungee Hairline" },
                                { "caption": "Codystar", "value": "Codystar" },
                                { "caption": "Comfortaa", "value": "Comfortaa" },
                                { "caption": "Cookie", "value": "Cookie" },
                                { "caption": "Dancing Script", "value": "Dancing Script" },
                                { "caption": "Gurajada", "value": "Gurajada" },
                                { "caption": "Hammersmith One", "value": "Hammersmith One" },
                                { "caption": "Julius Sans One", "value": "Julius Sans One" },
                                { "caption": "Major Mono Display", "value": "Major Mono Display" },
                                { "caption": "Medula One", "value": "Medula One" },
                                { "caption": "Open Sans", "value": "Open Sans" },
                                { "caption": "Orbitron", "value": "Orbitron" },
                                { "caption": "Roboto", "value": "Roboto" },
                                { "caption": "Roboto Condensed", "value": "Roboto Condensed" },
                                { "caption": "Saira Extra Condensed", "value": "Saira Extra Condensed" },
                                { "caption": "Share Tech Mono", "value": "Share Tech Mono" },
                                { "caption": "Space Mono", "value": "Space Mono" },
                                { "caption": "Syne Mono", "value": "Syne Mono" }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "List",
            "name": "dataEvents",
            "caption": "DataEvents",
            "rowCount": 3,
            "add": true,
            "delete": true,
            "edit": true,
            "columns": [
                {
                    "caption": "Name",
                    "name": "Name",
                    "width": "150px",
                    "add": "",
                    "edit": {
                        "type": "ValidationTextBox"
                    }
                },
                {
                    "caption": "Type",
                    "name": "Type",
                    "width": "100px",
                    "add": "module",
                    "edit": {
                        "type": "Select",
                        "options": [
                            {
                                "caption": "ICAL Modul",
                                "value": "module"
                            }
                        ]
                    }
                },
                {
                    "caption": "ICal Modul Instance",
                    "name": "moduleInstance",
                    "width": "400px",
                    "add": 0,
                    "edit": {
                        "type": "Select",
                        "options": [
                            {
                                "caption": "NoModuleFound",
                                "value": 0
                            }
                        ]
                    }
                },
                {
                    "caption": "Color",
                    "name": "Color",
                    "width": "100px",
                    "add": 0,
                    "edit": {
                        "type": "SelectColor"
                    }
                },
                {
                    "caption": "Color Alpha",
                    "name": "Color_Alpha",
                    "width": "50px",
                    "visible": false,
                    "add": 1.0,
                    "edit": {
                        "type": "HorizontalSlider",
                        "stepSize": 0.05,
                        "maximum": 1,
                        "minimum": 0
                    }
                },
                {
                    "caption": "Text Color",
                    "name": "textColor",
                    "width": "100px",
                    "add": 0,
                    "edit": {
                        "type": "SelectColor"
                    }
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Expert",
            "viewlevel": 2,
            "items": [
                { "type": "CheckBox", "name": "Debug", "caption": "Debug"},
                { "type": "CheckBox", "name": "EnableCache", "caption": "Enable Cache"},
                { "type": "CheckBox", "name": "CreateOutput", "caption": "Create HTMLBox"},
                {
                    "type": "RowLayout",
                    "items": [
                        { "type": "CheckBox", "name": "CreateOutput", "caption": "Create IPSView HTMLBox"},
                        { "type": "Label", "caption": "Works only if the Option IPSView in Gateway is enabled!"}
                    ]
                },
                { "type": "SelectScript", "name": "TemplateScriptID", "caption": "Template Script"},
                { "type": "CheckBox", "name": "EnableViewport", "caption": "Viewport Enable"},
                { "type": "NumberSpinner", "name": "IFrameHeight", "caption": "IFrame Height", "suffix": "px", "minimum": 0 },
                {
                    "type": "RowLayout",
                    "items": [
                        { "type": "Label", "caption": "Override Size"},
                        { "type": "NumberSpinner", "name": "overrideWidth", "caption": "Width", "suffix": "px", "minimum": 0 },
                        { "type": "NumberSpinner", "name": "overrideHeight", "caption": "Height", "suffix": "px", "minimum": 0 }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        { "type": "Label", "caption": "Custom-CSS"},
                        { "type": "SelectFile", "name": "CustomCSS", "caption": "File"},
                        { "type": "Button", "label": "Show", "onClick": " echo SymconJSLiveCalendar_GetCSSLink($id);"},
                        { "type": "Button", "label": "Show Default", "onClick": " echo SymconJSLiveCalendar_GetDefaultCSSLink($id);"}
                    ]
                }
            ]
        },
        { "type": "Select", "name": "ViewLevel", "caption": "View Level",
            "options": [
                { "caption": "Basic", "value": 0 },
                { "caption": "Advance", "value": 1 },
                { "caption": "Expert", "value": 2 }
            ]
        }
    ],
    "actions":
    [
        { "type": "Button", "label": "Get Link", "onClick": " echo SymconJSLiveCalendar_GetLink($id);" },
        {
            "type": "ExpansionPanel",
            "caption": "Configuration",
            "items": [
                { "type": "Label", "caption": "Load from other Modul"},
                {
                    "type": "RowLayout",
                    "items": [
                        {"type": "SelectObject", "name": "ObjectID", "caption": "Modul"},
                        { "type": "Button", "label": "Load", "onClick": " echo SymconJSLiveCalendar_LoadOtherConfiguration($id, $ObjectID);"}
                    ]
                },
                { "type": "Label", "caption": "Load from File"},
                {
                    "type": "RowLayout",
                    "items": [
                        {"type": "SelectFile", "name": "Config_fileData", "caption": "File"},
                        { "type": "Button", "label": "Load", "onClick": " echo SymconJSLiveCalendar_LoadConfigurationFile($id, $Config_fileData, true);"}
                    ]
                },
                { "type": "Button", "label": "Export", "onClick": " echo SymconJSLiveCalendar_GetConfigurationLink($id, true);" }
            ]
        }
    ],
    "status":
    [
        { "code": 102, "icon": "active", "caption": "Status OK!" }
    ]
}