{
    "elements":
    [   
        {
        "type": "ExpansionPanel",
        "caption": "Configuration",
        "expanded": false,
        "items": [
            { 
                "type": "RowLayout",
                "items": [
                    { 
                        "type": "ValidationTextBox", 
                        "name": "ForumUsername", 
                        "caption": "Forum Username" 
                    }, {
                        "type": "Button",
                        "caption": "Check",
                        "onClick": "echo SymconJSLiveConfigStore_GetForumUser($id, $ForumUsername);"
                    }
                ]
            }, { 
                "type": "RowLayout",
                "items": [
                    { 
                        "type": "PasswordTextBox", 
                        "name": "UserID", 
                        "caption": "UserID" 
                    }, {
                        "type": "Button",
                        "caption": "Show UserID",
                        "onClick": "echo SymconJSLiveConfigStore_ReadUserID($id);"
                    }
                ]
            }
        ]
    }],
    "actions":
    [
        {
            "type": "ExpansionPanel",
            "caption": "Upload Configuration",
            "name": "UploadConfiguartion", 
            "width": "1710px",
            "expanded": false,
            "items": [
                { 
                    "type": "RowLayout",
                    "items": [
                        { "type": "SelectInstance", 
                            "name": "PropertyInstanceID", 
                            "caption": "Target" 
                        }, { 
                            "type": "Select", 
                            "name": "ViewLevelRequire", 
                            "caption": "min. ViewLevel (Require)", 
                            "width": "160px",
                            "options": [
                                { "caption": "Simple", "value": 0 },
                                { "caption": "Basic", "value": 1 },
                                { "caption": "Advance", "value": 2 },
                                { "caption": "Expert", "value": 3 },
                                { "caption": "Developer", "value": 4 }
                            ]
                        }, { 
                            
                            "type": "ValidationTextBox", 
                            "width": "800px",
                            "name": "Description", 
                            "caption": "Description" 
                        },{ 
                            "type": "SelectFile", 
                            "width": "200px",
                            "name": "PropertyStringFile", 
                            "caption": "Picture", 
                            "extensions": ".jpg,.gif,.txt" 
                        }, {
                            "width": "80px",
                            "type": "Button",
                            "caption": "Upload",
                            "onClick": "TM_Update2($id);"
                        }
                    ]
                }
            ]
        },
        { 
            "type": "RowLayout",
            "items": [
                {
                    "type": "ExpansionPanel",
